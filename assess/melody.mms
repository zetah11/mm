it = 4 (mel, (mel | arp), (mel | arp | bss))

mel = 1/15 (fst, fst, fst, thn)       -- thirds
arp = 1/5 (dmin, fmaj, gmaj, bees)-1  -- fifths
bss = (D, F, G-1, A#-1)-3             -- bass

dmin = D-1, A-1, D, A, F
fmaj = F-1, A-1, F, A, F
gmaj = G-1, G, D, G-1, D
bees = G-1, A#-1, D, A#, A

fst = faf, faf, faf, faf, dad
thn = gbg, gbg, fbf, ebe, cac

faf = F, A, F+1
dad = D, A, D+1
gbg = G, A#, G+1
fbf = F, A#, F+1
ebe = E, A#, E+1
cac = C#, A, C#+1